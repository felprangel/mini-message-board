<html>
  <body>
    <h1><%= title %></h1>
    <% messages.forEach(message => { %>
    <ul>
      <li><%= message.text %></li>
      <li><%= message.user %></li>
      <li><%= message.added %></li>
    </ul>
    <% }) %>
    <button onclick="window.location.href = `${window.location.href}new`">
      Nova mensagem
    </button>
  </body>
</html>
